{
	"name": "AniList Discord Search Bot",
	"build": {
		"dockerfile": "Dockerfile",
		// Update 'VARIANT' to pick a Bun version.
		"args": {
			"VARIANT": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"GitHub.copilot",
				"eamodio.gitlens",
				"rangav.vscode-thunder-client"
			]
		}
	}
}
